{
  "name": "my-nextjs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build-storybook": "storybook build",
    "build": "next build",
    "commit": "cz",
    "dev": "next dev",
    "format": "prettier --write .",
    "lint:fix": "next lint --fix",
